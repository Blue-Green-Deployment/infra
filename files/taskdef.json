{
  "family": "swiggy-clone",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "REPLACED_IN_BUILD",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "REPLACED_IN_BUILD",
      "essential": true,
      "portMappings": [{ "containerPort": 8080, "hostPort": 8080 }]
    }
  ]
}
